# Proton Git LFS environment configuration
# This project is pre-production. Do not use for production data.

# Adapter configuration
SDK_SERVICE_URL=http://localhost:3000
PROTON_LFS_BACKEND=local
ADAPTER_ALLOW_MOCK_TRANSFERS=false
PROTON_LFS_LOCAL_STORE_DIR=
# Legacy fallback only. Prefer PROTON_PASS_* references.
PROTON_USERNAME=
PROTON_PASSWORD=
PROTON_TEST_USERNAME=
PROTON_TEST_PASSWORD=
PROTON_PASS_CLI_BIN=pass-cli
PROTON_PASS_REF_ROOT=pass://Personal/Proton Git LFS
PROTON_PASS_USERNAME_REF=pass://Personal/Proton Git LFS/username
PROTON_PASS_PASSWORD_REF=pass://Personal/Proton Git LFS/password
# Optional: run integration tests against an externally running SDK service.
PROTON_SDK_SERVICE_URL=
DEBUG=false

# SDK service configuration
SDK_SERVICE_PORT=3000
# local = deterministic local persistence, real = C# Proton SDK bridge
SDK_BACKEND_MODE=local
SDK_ALLOW_MOCK_MODE=false

# Proton API integration (for future real SDK wiring)
PROTON_API_BASE=https://drive.proton.me
PROTON_APP_VERSION=external-drive-protonlfs@dev
PROTON_DATA_PASSWORD=
PROTON_SECOND_FACTOR_CODE=
PROTON_REAL_BRIDGE_BIN=
PROTON_REAL_BRIDGE_PROJECT=proton-sdk-service/tools/proton-real-bridge/ProtonRealBridge.csproj
PROTON_REAL_BRIDGE_CONFIGURATION=Release
PROTON_REAL_BRIDGE_TIMEOUT_MS=300000
PROTON_REAL_BRIDGE_TIMEOUT_SECONDS=300

# Storage layout
LFS_STORAGE_BASE=LFS

# Logging
LOG_LEVEL=info
LOG_FILE=

# Timeouts (seconds)
TRANSFER_TIMEOUT=300
API_TIMEOUT=30
